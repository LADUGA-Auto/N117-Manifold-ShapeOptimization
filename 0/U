    }

    OUTLET_2
    {
        //type            zeroGradient;
        type            inletOutlet;
        inletValue      uniform (0 0 0);
    }

    OUTLET_1
    {
        //type            zeroGradient;
        type            inletOutlet;
        inletValue      uniform (0 0 0);
    }

    WALL_TANK
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    WALL_INLET_CHANNEL
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    WALL_CHANNEL_4
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    WALL_CHANNEL_3
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    WALL_CHANNEL_2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    WALL_CHANNEL_1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }    

    SIDE_4
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    SIDE_3
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    SIDE_2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    SIDE_1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    frontAndBack
